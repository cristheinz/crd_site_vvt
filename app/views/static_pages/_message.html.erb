<div class="row-fluid">
		<p class="lead"><i><%=profile.msg1%></i></p>
		<p class="msg1"><%=profile.msg2%></p>
		<div class="row-fluid">
			<div class="msg2 pull-right">
				<strong>
				<p><%="#{t(:phone)} #{profile.tel1}"%> <%= " / #{profile.tel2}" unless profile.tel2.to_s.empty? %></p>
				<%= "<p>#{t(:emergency)}  #{profile.tel3}</p>".html_safe unless profile.tel3.to_s.empty? %>
				</strong>
			</div>
			<div class="span6">
				<%=render 'layouts/skype'%>
			</div>
		</div>
</div>
